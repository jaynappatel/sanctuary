{
  "name": "sanctuary-austin",
  "version": "1.0.0",
  "private": true,
  "description": "A community resource hub connecting people and animals in Austin, TX to essential services and support",
  "author": {
    "name": "Sanctuary Austin Team",
    "email": "support@sanctuaryaustin.org"
  },
  "keywords": [
    "austin",
    "community resources",
    "emergency help",
    "food banks",
    "shelters",
    "legal aid",
    "pet services",
    "nonprofit"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate-types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "analyze": "cross-env ANALYZE=true next build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@types/google.maps": "^3.55.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.312.0",
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanctuary-austin/website.git"
  },
  "bugs": {
    "url": "https://github.com/sanctuary-austin/website/issues"
  },
  "homepage": "https://sanctuaryaustin.org",
  "license": "MIT"
}
